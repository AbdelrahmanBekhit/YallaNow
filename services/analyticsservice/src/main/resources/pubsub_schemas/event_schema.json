{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Item",
  "type": "object",
  "properties": {
    "operation": {
      "type": "string"
    },
    "eventId": {
      "type": "string"
    },
    "groupId": {
      "type": "string"
    },
    "groupName": {
      "type": "string"
    },
    "eventTitle": {
      "type": "string"
    },
    "eventDescription": {
      "type": "string"
    },
    "startTime": {
      "type": "string",
      "format": "date-time"
    },
    "endTime": {
      "type": "string",
      "format": "date-time"
    },
    "location": {
      "type": "object",
      "properties": {
        "addressID": {
          "type": "integer"
        },
        "street": {
          "type": "integer"
        },
        "city": {
          "type": "string"
        },
        "province": {
          "type": "string"
        },
        "country": {
          "type": "string"
        }
      },
      "required": ["addressID", "street", "city", "province", "country"]
    },
    "eventType": {
      "type": "string"
    },
    "attendeeCount": {
      "type": "integer",
      "minimum": 0
    },
    "capacity": {
      "type": "integer"
    },
    "status": {
      "type": "string"
    }
  },
  "required": ["eventId", "groupName", "eventTitle", "startTime", "endTime", "location", "eventType", "attendeeCount"]
}
